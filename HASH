	private: System::Void label7_Click(System::Object^ sender, System::EventArgs^ e) {
	}
		   int hash_func(int key, int size) {
			   return floor(size * (key * A - floor(key * A)));
		   }

	private: System::Void button1_Click(System::Object^ sender, System::EventArgs^ e) {
		static string item[5];
		int flag = 1;
		int hash;
		item[0] = msclr::interop::marshal_as<std::string>(textBox1->Text);
		item[1] = msclr::interop::marshal_as<std::string>(textBox2->Text);
		item[2] = msclr::interop::marshal_as<std::string>(textBox3->Text);
		item[3] = msclr::interop::marshal_as<std::string>(textBox4->Text);
		item[4] = msclr::interop::marshal_as<std::string>(textBox5->Text);
		noi++;
		for (int i = 0; i < noi; i++) {
			if (ids[i] == stoi(item[0]))
				flag = 0;
		}
		if (flag == 0) {
			label29->Text = gcnew String("Item already exists!");
			textBox1->Clear();
			textBox2->Clear();
			textBox3->Clear();
			textBox4->Clear();
			textBox5->Clear();
		}
		else {
			hash = hash_func(stoi(item[0]), 100);
			while (1) {
				if (hashtable[hash][0] == 100000) {
					fstream f1("rec.txt", ios::app);
					f1.seekp(0, ios::end);
					hashtable[hash][0] = stoi(item[0]);
					hashtable[hash][1] = f1.tellp();
					f1 << item[0] << "|" << item[1] << "|" << item[2] << "|" << item[3] << "|" << item[4] << "|";
					f1.close();
					label29->Text = gcnew String("Item added!");
					textBox1->Clear();
					textBox2->Clear();
					textBox3->Clear();
					textBox4->Clear();
					textBox5->Clear();
					ids[noi - 1] = stoi(item[0]);
					break;
				}
				else {
					hash++;
				}
			}
		}
	}
	private: System::Void button5_Click(System::Object^ sender, System::EventArgs^ e) {
		for (int i = 0; i < 100; i++) {
			hashtable[i][0] = 100000;
			hashtable[i][1] = 100000;
		}
		fstream f1("rec.txt", ios::out);
		f1 << "";
		f1.close();
	}
	private: System::Void button2_Click(System::Object^ sender, System::EventArgs^ e) {
		string itemid;
		char id[10], name[30], price[10], mfgdate[30], expdate[30];
		itemid = msclr::interop::marshal_as<std::string>(textBox6->Text);
		int flag = 0, hash, pointer;
		hash = hash_func(stoi(itemid), 100);
		while (hash < 100) {
			if (hashtable[hash][0] == stoi(itemid)) {
				pointer = hashtable[hash][1];
				fstream f1("rec.txt", ios::in);
				f1.seekg(pointer);
				f1.getline(id, 10, '|');
				f1.getline(name, 30, '|');
				f1.getline(price, 10, '|');
				f1.getline(mfgdate, 30, '|');
				f1.getline(expdate, 30, '|');
				label39->Text = gcnew String(id);
				label38->Text = gcnew String(name);
				label37->Text = gcnew String(price);
				label36->Text = gcnew String(mfgdate);
				label35->Text = gcnew String(expdate);
				textBox6->Clear();
				string checkid(strlen(id), ' ');
				if (strcmp(id, checkid.c_str()) == 0)
					label13->Text = gcnew String("Item not found!");
				else
					label13->Text = gcnew String("Item found!");
				flag = 1;
				break;
			}
			else {
				hash++;
			}
		}
		if (flag == 0) {
			textBox6->Clear();
			label13->Text = gcnew String("Item not found!");
			label39->Text = gcnew String("               ");
			label38->Text = gcnew String("               ");
			label37->Text = gcnew String("               ");
			label36->Text = gcnew String("               ");
			label35->Text = gcnew String("               ");
		}
	}
	private: System::Void button3_Click(System::Object^ sender, System::EventArgs^ e) {
		string itemid, buff;
		char id[10], name[30], price[10], mfgdate[30], expdate[30];
		itemid = msclr::interop::marshal_as<std::string>(textBox11->Text);
		int flag = 0, hash, pointer, len;
		hash = hash_func(stoi(itemid), 100);
		while (hash < 100) {
			if (hashtable[hash][0] == stoi(itemid)) {
				pointer = hashtable[hash][1];
				fstream f1("rec.txt", ios::in);
				f1.seekg(pointer);
				f1.getline(id, 30, '|');
				f1.getline(name, 30, '|');
				f1.getline(price, 30, '|');
				f1.getline(mfgdate, 30, '|');
				f1.getline(expdate, 30, '|');
				len = strlen(id) + strlen(name) + strlen(price) + strlen(mfgdate) + strlen(expdate) + 5;
				string replace(len, ' ');
				if (strcmp(id, itemid.c_str()) == 0) {
					flag = 1;
					fstream f2("rec.txt", ios::in);
					f2 >> buff;
					f2.close();
					buff.replace(pointer, len, replace);
					fstream f3("rec.txt", ios::out);
					f3 << buff;
					f3.close();
					textBox11->Clear();
					label12->Text = gcnew String("Item deleted!");
				}
				break;
			}
			else {
				hash++;
			}
		}
		if (flag == 0) {
			textBox11->Clear();
			label12->Text = gcnew String("Item does not exist!");
		}
		for (int i = 0; i < noi; i++) {
			if (ids[i] == stoi(itemid)) {
				ids[i] = 0;
			}
		}
	}
	private: System::Void button4_Click(System::Object^ sender, System::EventArgs^ e) {
		string itemid[5], itemnames[5], itemprices[5];
		char id[10], name[30], price[10], mfgdate[30], expdate[30];
		int hash, pointer, total = 0;
		string quantity[5];
		itemid[0] = msclr::interop::marshal_as<std::string>(textBox16->Text);
		itemid[1] = msclr::interop::marshal_as<std::string>(textBox15->Text);
		itemid[2] = msclr::interop::marshal_as<std::string>(textBox14->Text);
		itemid[3] = msclr::interop::marshal_as<std::string>(textBox13->Text);
		itemid[4] = msclr::interop::marshal_as<std::string>(textBox7->Text);
		if (itemid[0] != "") {
			hash = hash_func(stoi(itemid[0]), 100);
			while (hash < 100) {
				if (hashtable[hash][0] == stoi(itemid[0])) {
					pointer = hashtable[hash][1];
					fstream f1("rec.txt", ios::in);
					f1.seekg(pointer);
					f1.getline(id, 10, '|');
					f1.getline(name, 30, '|');
					f1.getline(price, 10, '|');
					f1.getline(mfgdate, 30, '|');
					f1.getline(expdate, 30, '|');
					textBox16->Clear();
					label18->Text = gcnew String(name);
					label23->Text = gcnew String(price);
					quantity[0] = msclr::interop::marshal_as<std::string>(textBox17->Text);
					label43->Text = gcnew String(quantity[0].c_str());
					total = total + (stoi(price) * stoi(quantity[0]));
					itemnames[0] = name;
					itemprices[0] = price;
					textBox7->Clear();
					textBox17->Clear();
					break;
				}
				else {
					hash++;
				}
			}
		}
		if (itemid[1] != "") {
			hash = hash_func(stoi(itemid[1]), 100);
			while (hash < 100) {
				if (hashtable[hash][0] == stoi(itemid[1])) {
					pointer = hashtable[hash][1];
					fstream f1("rec.txt", ios::in);
					f1.seekg(pointer);
					f1.getline(id, 10, '|');
					f1.getline(name, 30, '|');
					f1.getline(price, 10, '|');
					f1.getline(mfgdate, 30, '|');
					f1.getline(expdate, 30, '|');
					textBox15->Clear();
					label19->Text = gcnew String(name);
					label24->Text = gcnew String(price);
					quantity[1] = msclr::interop::marshal_as<std::string>(textBox12->Text);
					total = total + (stoi(price) * stoi(quantity[1]));
					label42->Text = gcnew String(quantity[1].c_str());
					itemnames[1] = name;
					itemprices[1] = price;
					textBox13->Clear();
					textBox12->Clear();
					break;
				}
				else {
					hash++;
				}
			}
		}
		if (itemid[2] != "") {
			hash = hash_func(stoi(itemid[2]), 100);
			while (hash < 100) {
				if (hashtable[hash][0] == stoi(itemid[2])) {
					pointer = hashtable[hash][1];
					fstream f1("rec.txt", ios::in);
					f1.seekg(pointer);
					f1.getline(id, 10, '|');
					f1.getline(name, 30, '|');
					f1.getline(price, 10, '|');
					f1.getline(mfgdate, 30, '|');
					f1.getline(expdate, 30, '|');
					textBox14->Clear();
					label20->Text = gcnew String(name);
					label25->Text = gcnew String(price);
					quantity[2] = msclr::interop::marshal_as<std::string>(textBox10->Text);
					total = total + (stoi(price) * stoi(quantity[2]));
					label41->Text = gcnew String(quantity[2].c_str());
					itemnames[2] = name;
					itemprices[2] = price;
					textBox14->Clear();
					textBox10->Clear();
					break;
				}
				else {
					hash++;
				}
			}
		}
		if (itemid[3] != "") {
			hash = hash_func(stoi(itemid[3]), 100);
			while (hash < 100) {
				if (hashtable[hash][0] == stoi(itemid[3])) {
					pointer = hashtable[hash][1];
					fstream f1("rec.txt", ios::in);
					f1.seekg(pointer);
					f1.getline(id, 10, '|');
					f1.getline(name, 30, '|');
					f1.getline(price, 10, '|');
					f1.getline(mfgdate, 30, '|');
					f1.getline(expdate, 30, '|');
					textBox13->Clear();
					label27->Text = gcnew String(name);
					label17->Text = gcnew String(price);
					quantity[3] = msclr::interop::marshal_as<std::string>(textBox9->Text);
					total = total + (stoi(price) * stoi(quantity[3]));
					label46->Text = gcnew String(quantity[3].c_str());
					itemnames[3] = name;
					itemprices[3] = price;
					textBox15->Clear();
					textBox9->Clear();
					break;
				}
				else {
					hash++;
				}
			}
		}
		if (itemid[4] != "") {
			hash = hash_func(stoi(itemid[4]), 100);
			while (hash < 100) {
				if (hashtable[hash][0] == stoi(itemid[4])) {
					pointer = hashtable[hash][1];
					fstream f1("rec.txt", ios::in);
					f1.seekg(pointer);
					f1.getline(id, 10, '|');
					f1.getline(name, 30, '|');
					f1.getline(price, 10, '|');
					f1.getline(mfgdate, 30, '|');
					f1.getline(expdate, 30, '|');
					textBox7->Clear();
					label26->Text = gcnew String(name);
					label15->Text = gcnew String(price);
					quantity[4] = msclr::interop::marshal_as<std::string>(textBox8->Text);
					total = total + (stoi(price) * stoi(quantity[4]));
					label45->Text = gcnew String(quantity[4].c_str());
					itemnames[4] = name;
					itemprices[4] = price;
					textBox16->Clear();
					textBox8->Clear();
					break;
				}
				else {
					hash++;
				}
			}
		}
		label14->Text = gcnew String(total.ToString());
	}
	};
}
